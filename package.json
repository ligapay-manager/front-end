{
  "name": "ligapay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bundle": "node node_modules/react-native/local-cli/cli.js start",
    "bundle:reset": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "run:android": "react-native run-android",
    "test": "jest",
    "lint": "eslint **/*.js",
    "lint:fix": "prettier-eslint \"**/*.js\"",
    "start": "concurrently --kill-others-on-fail \"npm:bundle:reset\" \"npm:run:android\""
  },
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "react": "16.8.3",
    "react-native": "0.59.2",
    "react-native-gesture-handler": "^1.1.0",
    "react-navigation": "^3.6.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "babel-jest": "^24.5.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "metro-react-native-babel-preset": "^0.53.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx}": [
        "eslint --fix",
        "prettier-eslint",
        "git add"
      ]
    }
  }
}